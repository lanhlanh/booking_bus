<% provide :title, t("user.user_info") %>
<div class="title-info-page">
  <%= t "user.user_info" %>
</div>
<div class="body-info-page">
  <div class="container">
    <div class="col-md-4 user-info-div">
      <%= render "users/user_detail_info", user: @user %>
    </div>
    <div class="col-md-8 history-of-user">
      <%= render "users/user_history", user: @user %>
    </div>
  </div>
</div>
